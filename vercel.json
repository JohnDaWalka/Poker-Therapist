{
  "version": 2,
  "name": "poker-therapist",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "XAI_API_KEY": "@xai_api_key",
    "OPENAI_API_KEY": "@openai_api_key",
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "GOOGLE_API_KEY": "@google_api_key",
    "GOOGLE_AI_API_KEY": "@google_ai_api_key",
    "PERPLEXITY_API_KEY": "@perplexity_api_key",
    "AUTHORIZED_EMAILS": "@authorized_emails",
    "JWT_SECRET_KEY": "@jwt_secret_key",
    "AZURE_TENANT_ID": "@azure_tenant_id",
    "AZURE_CLIENT_ID": "@azure_client_id",
    "AZURE_CLIENT_SECRET": "@azure_client_secret",
    "AZURE_AUTHORITY": "@azure_authority",
    "GOOGLE_CLOUD_PROJECT_ID": "@google_cloud_project_id",
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "GOOGLE_STORAGE_BUCKET_NAME": "@google_storage_bucket_name",
    "GOOGLE_APPLICATION_CREDENTIALS": "@google_application_credentials",
    "APPLE_TEAM_ID": "@apple_team_id",
    "APPLE_SERVICES_ID": "@apple_services_id",
    "APPLE_KEY_ID": "@apple_key_id",
    "APPLE_PRIVATE_KEY_PATH": "@apple_private_key_path",
    "INSTITUTIONAL_EMAIL_DOMAIN": "@institutional_email_domain"
  }
}
